{
  "name": "georgebohnisch/laravel-elasticsearch-query-builder",
  "description": ":Query Elasticsearch by using Eloquent",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Shisun(Leo) Xia",
      "email": "shisun.xia@hotmail.com"
    }
  ],
  "homepage": "https://github.com/shisun/laravel-elasticsearch-query-builder",
  "keywords": [
    "Laravel",
    "LaravelElasticsearchQueryBuilder",
    "elasticsearch"
  ],
  "require": {
    "illuminate/database": "~7.0|~8.0",
    "elasticsearch/elasticsearch": "^6.0.1",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Shisun\\LaravelElasticsearchQueryBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shisun\\LaravelElasticsearchQueryBuilder\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Shisun\\LaravelElasticsearchQueryBuilder\\LaravelElasticsearchQueryBuilderServiceProvider"
      ],
      "aliases": {
        "LaravelElasticsearchQueryBuilder": "Shisun\\LaravelElasticsearchQueryBuilder\\Facades\\LaravelElasticsearchQueryBuilder"
      }
    }
  }
}
